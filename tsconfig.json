{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2015"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "paths": {
      "*": ["*", "src/*"]
    }
  }
}
